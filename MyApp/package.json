{
  "name": "MyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npm run build-vendor && npm run build",
    "dev": "webpack --config webpack.config.js --watch",
    "dev-server": "webpack-dev-server",
    "build": "webpack --config webpack.config.js",
    "build-prod": "webpack --config webpack.config.js --env.prod",
    "build-vendor": "webpack --config webpack.config.vendor.js",
    "publish": "webpack --config webpack.config.vendor.js --env.prod && npm run build-prod",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "dtos-update": "cd src && typescript-ref"
  },
  "jest": {
    "globals": {
      "BaseUrl": "http://localhost:5000"
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|sass)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test.fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/src/test.shim.js",
      "<rootDir>/src/test.setup.js"
    ]
  },
  "dependencies": {
    "@servicestack/client": "^1.0.31",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "es6-shim": "^0.35.5",
    "font-awesome": "^4.7.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@servicestack/cli": "^1.0.10",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.50",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "add-asset-html-webpack-plugin": "^2.1.3",
    "autoprefixer": "^7.2.6",
    "awesome-typescript-loader": "^3.5.0",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "globule": "^1.2.1",
    "gulp": "^3.9.1",
    "gulp-msbuild": "^0.5.5",
    "gulp-newer": "^1.4.0",
    "gulp-nuget-pack": "^0.0.7",
    "gulp-nuget-restore": "^0.6.0",
    "gulp-util": "^3.0.7",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.6.0",
    "imports-loader": "^0.7.1",
    "jest": "^22.4.4",
    "node-sass": "^4.12.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^2.1.6",
    "precss": "^3.1.2",
    "react-test-renderer": "^16.9.0",
    "run-sequence": "^2.2.1",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.20.3",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^3.1.11"
  }
}
